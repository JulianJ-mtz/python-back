{
  "version": 2,
  "buildCommand": "pip install poetry && poetry install",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "api/index.py": {
      "memory": 512,
      "maxDuration": 30
    }
  }
}